<h1>Enter Your User Info: </h1>

<%@user.errors.full_messages.each do |msg|%>
    <%=msg%>
<%end%>

<%=form_for @user do |f|%>
<%= f.label "Enter your name first & last name"%>
<%= f.text_field :name%><br>

<%= f.label "Enter your user name here"%>
<%= f.text_field :username%><br>
<%=f.submit%>
<%end%>